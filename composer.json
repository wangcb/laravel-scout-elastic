{
    "name": "wangcb/laravel-scout-elastic",
    "description": "elastic-drives",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wangcb\\LaravelScoutElastic\\": "src/"
        }
    },
    "keywords": [
        "laravel",
        "scout",
        "elasticsearch",
        "elastic"
    ],
    "authors": [
        {
            "name": "wangcb615",
            "email": "wangcb615@163.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "laravel/scout": "^9.3"
    },
    "require-dev": {
        "elasticsearch/elasticsearch": "^7.9"
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.9.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wangcb\\LaravelScoutElastic\\LaravelScoutElasticProvider"
            ]
        }
    },
    "prefer-stable": true
}
